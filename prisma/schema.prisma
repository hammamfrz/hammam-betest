generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URL")
}

model User {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model Users {
  id             String    @id @default(auto()) @map("_id") @db.ObjectId
  accountNumber  String
  createdAt      DateTime? @db.Date
  emailAddress   String
  identityNumber String
  password       String
  updatedAt      DateTime? @db.Date
  userName       String
}
